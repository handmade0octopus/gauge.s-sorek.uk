//	"deleteAfterUpload": true,	// Deletes files after succesfully uploading to dropbox
	"snake": true,
	"displayRows": 2,
	"displayColumns": 2,
	"noMoreEntries": true,
	"obdRefresh": 80,	// default is 20; uncomment if you are sure your ECU will accept this speed!
