			"noLog": true
		},
		{
			"header": "Rear Tire Aspect Ratio",
			"expr": "40",
			"hidden": true,
			"noLog": true
		},
		{
			"header": "Rear Wheel Diameter",
			"unit": "in",
			"expr": "18",
			"hidden": true,
			"noLog": true
		},
		{
			"header": "Final Drive Ratio",
			"expr": "3.62",
			"hidden": true,
			"noLog": true
		},
		{
			"header": "CAN Raw to KPH Constant",
			"expr": "0.0000977",
			"hidden": true,
			"noLog": true
		},
		{
			"header": "CAN Raw to MPH Constant",
			"expr": "0.0000607",
			"hidden": true,
			"noLog": true
