		{
			"header": "Intake Air Temp",
			"unit": "°C",
			"offset": 22,
			"mul": 0.75,
			"add": -48
		},
		{
			"header": "Coolant Temp",
			"unit": "°C",
			"offset": 23,
			"mul": 0.75,
			"add": -48
		},
		{
			"header": "Speed",
			"unit": "km/h",
			"offset": 26,
			"dec": 0,
			"mul": 1.25
