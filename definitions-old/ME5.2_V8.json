	"ecuparam": [
		{
			"header": "Economy",
			"unit": "l/100km",
			"expr": "clip(100*x/(y+0.1))",
			"x": "Fuel usage",
			"y": "Speed"
		},
		{
			"header": "Econ avg 1",
			"unit": "l/100km",
			"expr": "avg(\"Econ avg 1\",x, y)",
			"x": "Economy",
			"y": "Speed",
			"avgLen": 20.0
		},
		{
			"header": "Econ avg 2",
			"unit": "l/100km",
			"expr": "avg(\"Econ avg 2\",x, y)",
			"x": "Economy",
			"y": "Speed",
			"avgLen": 20.0
		},
		{
			"header": "Trip 1",
			"unit": "km",
			"expr": "trip(\"Trip 1\",x)",
			"x": "Speed"
		},
		{
			"header": "Trip 2",
			"unit": "km",
			"expr": "trip(\"Trip 2\",x)",
			"x": "Speed"
		},
		{
			"header": "Fuel usage",
			"unit": "l/h",
			"expr": "(x/60)*(y/1000)*(250/16.6667)*3",  // Change 328i 250cc/min to 180cc/min for green injectors 320i/323i/325i, 280cc for 330i
			"x": "Engine Speed",
			"y": "Fuel Inj"
		},
		{
			"header": "Engine Speed",
			"unit": "RPM",
			"offset": 18,
			"length": 1,
			"mul": 40.0,
			"max": 7000,	
			"dec": 0,
			"flashPoint": 6500
		},
		{
			"header": "Engine Load",
			"unit": "mg/str",
			"offset": 21,
			"length": 1,
			"mul": 0.5,
			"dec": 1
		},
		{
			"header": "Mass Airflow",
			"unit": "kg/h",
			"offset": 0,
			"length": 2,
			"mul": 0.25,
			"dec": 1
		},
		{
			"header": "TPS",
			"unit": "%",
			"offset": 19,
			"mul": 0.5,
			"max": 100,
			"min": 0,
			"dec": 0				// Remember about this comma when editing line below
		//	"autoLogPoint": 60.0	// You can comment out or delete if you don't want to log on WOT.
